<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dk.aau.cs.psylog.survey_library" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="dk.aau.cs.psylog.sensor.survey_library.TestScheduler"
            android:label="@string/title_activity_test_scheduler" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="dk.aau.cs.psylog.sensor.survey_library.Scheduler"
            android:enabled="true" />

        <activity
            android:name="dk.aau.cs.psylog.sensor.survey_library.TransparentActivity"
            android:label="@string/title_activity_transparent"
            android:theme="@style/Theme.Transparent">
        </activity>
    </application>

</manifest>
